    "locc",
    "println",
    "puppycrawl",
    "pystr",
    "sdkman",
    "sphinxcontrib",
    "talsmasoftware",
    "umldoclet",
    "undict",
    "werror",
    "xdoclint",
    "xlint",
    "dmmdc",
    "dollarmath",
    "pdfcrop",
    "smartquotes"
